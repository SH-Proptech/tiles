{
  "type": "job",
  "name": "tiles-seed",
  "image": {
    "repo": "propacr.azurecr.io",
    "name": "tiles",
    "tag": "latest"
  },
  "container": {
    "backoffLimit": 0
  },
  "op": {
    "item": "tiles"
  },
  "storage": {
    "enabled": true
  },
  "args": [
    "cache",
    "seed",
    "--config",
    "/tegola/config.toml",
    "--logger",
    "zap"
  ]
}
