{
  "type": "deployment",
  "name": "tiles",
  "domain": {
    "host": "propmatch.co.uk",
    "subdomain": "tiles",
    "production_ssl": true
  },
  "image": {
    "repo": "propacr.azurecr.io",
    "name": "tiles",
    "tag": "latest"
  },
  "container": {
    "replicaCount": 2,
    "port": 8080,
    "path": "/",
    "healthEndpoint": "/capabilities/properties.json"
  },
  "op": {
    "item": "prop-tiles"
  },
  "args": ["serve", "--config", "/tegola/config.toml"]
}
